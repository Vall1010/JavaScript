function main(){

moveRow();
left();
right();
left();
right();


}

function moveRow(){
move();   
move();
move();
move();
   }
   
   
function right(){
turnRight();
move();
turnRight();
moveRow();
}


function left(){
turnLeft();
move();
turnLeft();
moveRow();}
